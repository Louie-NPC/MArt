                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <ImageView
                            android:id="@+id/imageRideletebinfive"
                            android:layout_gravity="center_vertical"
                            android:layout_height="@dimen/_30pxh"
                            android:layout_width="@dimen/_30pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_ri_delete_bin_5_line"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_ri_delete_bin_5_line"/>
                        <TextView android:alpha="0.9"
                            android:ellipsize="end"
                            android:gravity="bottom"
                            android:id="@+id/txtFreeupspace"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_3pxv"
                            android:layout_marginStart="@dimen/_34pxh"
                            android:layout_marginTop="@dimen/_6pxv"
                            android:layout_width="match_parent"
                            android:singleLine="true"

                            style="@style/txtIntersemibold16_1" tools:text="@string/lbl_free_up_space"/>
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/linearRowicoutlinedat"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageIcoutlinedata"
                            android:layout_gravity="center_vertical"
                            android:layout_height="@dimen/_30pxh"
                            android:layout_width="@dimen/_30pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_ic_outline_data_exploration"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_ic_outline_data_exploration"/>
                        <TextView android:alpha="0.9"
                            android:ellipsize="end"
                            android:gravity="center_vertical"
                            android:id="@+id/txtDatasaver"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_4pxv"
                            android:layout_marginStart="@dimen/_34pxh"
                            android:layout_marginTop="@dimen/_5pxv"
                            android:layout_width="match_parent"
                            android:singleLine="true"

                            style="@style/txtIntersemibold16_1" tools:text="@string/lbl_data_saver"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnactions"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_5pxv"
                android:layout_marginStart="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_8pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtActions"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_12pxh"
                    android:layout_width="wrap_content"
                    android:singleLine="true"

                    style="@style/txtIntersemibold16" tools:text="@string/lbl_actions"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumnicsharp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_6pxv"
                    android:layout_width="match_parent"
                    android:orientation="vertical"
                    android:paddingBottom="@dimen/_32pxv"
                    android:paddingEnd="@dimen/_24pxh"
                    android:paddingStart="@dimen/_24pxh"
                    android:paddingTop="@dimen/_32pxv" style="@style/groupStylegray_800cornerRadius">
                    <LinearLayout android:id="@+id/linearRowicsharpOne"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageIcsharpOne"
                            android:layout_gravity="center_vertical"
                            android:layout_height="@dimen/_30pxh"
                            android:layout_width="@dimen/_30pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_ic_sharp_outlined_flag"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_ic_sharp_outlined_flag"/>
                        <TextView android:ellipsize="end"
                            android:gravity="bottom"
                            android:id="@+id/txtReporta"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_3pxv"
                            android:layout_marginStart="@dimen/_32pxh"
                            android:layout_marginTop="@dimen/_6pxv"
                            android:layout_width="match_parent"
                            android:singleLine="true"

                            style="@style/txtIntersemibold16" tools:text="@string/msg_report_a_problem"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearRowmdilogout"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageMdilogoutOne"
                            android:layout_gravity="center_vertical"
                            android:layout_height="@dimen/_30pxh"
                            android:layout_width="@dimen/_30pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_mdi_logout"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_mdi_logout"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtLogout"
                            android:layout_gravity="bottom"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_3pxv"
                            android:layout_marginStart="@dimen/_32pxh"
                            android:layout_marginTop="@dimen/_6pxv"
                            android:layout_width="wrap_content"
                            android:singleLine="true"

                            style="@style/txtIntersemibold16" tools:text="@string/lbl_log_out"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
