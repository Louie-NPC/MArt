                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:gravity="start"
                    android:orientation="vertical"
                    android:paddingStart="@dimen/_22pxh"
                    android:paddingTop="@dimen/_32pxv"
                    android:paddingEnd="@dimen/_22pxh"
                    android:paddingBottom="@dimen/_32pxv">

                    <LinearLayout
                        android:id="@+id/linearRowmaterialsymb1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/imageMaterialsymbol1"
                            android:layout_width="@dimen/_30pxh"
                            android:layout_height="@dimen/_30pxh"
                            android:layout_gravity="center_vertical"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_material_symbol_white_a700"
                            tools:ignore="ContentDescription"
                            tools:src="@drawable/img_material_symbol_white_a700" />

                        <TextView
                            android:id="@+id/txtSecurity"
                            style="@style/txtIntersemibold16"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_32pxh"
                            android:layout_marginTop="@dimen/_6pxv"
                            android:layout_marginBottom="@dimen/_3pxv"
                            android:ellipsize="end"
                            android:gravity="bottom"

                            android:singleLine="true"
                            tools:text="@string/lbl_security" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/linearRowiconamoon"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/imageIconamoonOne"
                            android:layout_width="@dimen/_30pxh"
                            android:layout_height="@dimen/_30pxh"
                            android:layout_gravity="center_vertical"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_iconamoon_notification"
                            tools:ignore="ContentDescription"
                            tools:src="@drawable/img_iconamoon_notification" />

                        <TextView
                            android:id="@+id/txtNotifications"
                            style="@style/txtIntersemibold16"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_32pxh"
                            android:layout_marginTop="@dimen/_4pxv"
                            android:layout_marginBottom="@dimen/_5pxv"
                            android:ellipsize="end"
                            android:gravity="center_vertical"
                            android:singleLine="true"
                            tools:text="@string/lbl_notifications" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/linearRowicoutlineloc"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/imageIcoutlinelock"
                            android:layout_width="@dimen/_30pxh"
                            android:layout_height="@dimen/_30pxh"
                            android:layout_gravity="center_vertical"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_ic_outline_lock"
                            tools:ignore="ContentDescription"
                            tools:src="@drawable/img_ic_outline_lock" />

                        <TextView
                            android:id="@+id/txtPrivacy"
                            style="@style/txtIntersemibold16"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="bottom"
                            android:layout_marginStart="@dimen/_32pxh"
                            android:layout_marginTop="@dimen/_6pxv"
                            android:layout_marginBottom="@dimen/_3pxv"
                            android:ellipsize="end"
                            android:singleLine="true"
                            tools:text="@string/lbl_privacy" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnsupportab"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSupportabout"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_12pxh"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    style="@style/txtIntersemibold16" tools:text="@string/lbl_support_about"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumnmaterials1"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_width="match_parent"
                    android:orientation="vertical"
                    android:paddingBottom="@dimen/_12pxv"
                    android:paddingEnd="@dimen/_23pxh"
                    android:paddingStart="@dimen/_23pxh"
                    android:paddingTop="@dimen/_12pxv" style="@style/groupStylegray_800cornerRadius">
                    <LinearLayout
                        android:id="@+id/linearRowmaterialsymb2"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <ImageView
                            android:id="@+id/imageMaterialsymbol2"
                            android:layout_gravity="center_vertical"
                            android:layout_height="@dimen/_30pxh"
                            android:layout_width="@dimen/_30pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_material_symbol_white_a700_30x30"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_material_symbol_white_a700_30x30"/>
                        <TextView android:ellipsize="end"
                            android:gravity="bottom"
                            android:id="@+id/txtMysubscribtion"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_3pxv"
                            android:layout_marginStart="@dimen/_31pxh"
                            android:layout_marginTop="@dimen/_6pxv"
                            android:layout_width="match_parent"
                            android:singleLine="true"

                            style="@style/txtIntersemibold16" tools:text="@string/lbl_my_subscribtion"/>
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/linearRowmdiquestionm"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageMdiquestionma"
                            android:layout_gravity="center_vertical"
                            android:layout_height="@dimen/_30pxh"
                            android:layout_width="@dimen/_30pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_mdi_question_ma"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_mdi_question_ma"/>
                        <TextView android:ellipsize="end"
                            android:gravity="bottom"
